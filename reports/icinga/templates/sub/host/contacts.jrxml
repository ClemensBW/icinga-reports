<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="Host contacts sub report" language="groovy" pageWidth="555" pageHeight="842" columnWidth="555" leftMargin="0" rightMargin="0" topMargin="20" bottomMargin="0">
	<property name="ireport.zoom" value="1.5"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<style name="table">
		<box>
			<pen lineWidth="1.0" lineColor="#CCCCCC"/>
		</box>
	</style>
	<style name="table_TH" mode="Opaque" backcolor="#81AAB8">
		<box>
			<topPen lineWidth="0.5" lineColor="#CCCCCC"/>
			<bottomPen lineWidth="0.5" lineColor="#CCCCCC"/>
		</box>
	</style>
	<style name="table_CH" mode="Opaque" backcolor="#CFE0E6">
		<box>
			<topPen lineWidth="0.5" lineColor="#CCCCCC"/>
			<bottomPen lineWidth="0.5" lineColor="#CCCCCC"/>
		</box>
	</style>
	<style name="table_TD" mode="Opaque" backcolor="#FFFFFF">
		<box>
			<topPen lineWidth="0.5" lineColor="#CCCCCC"/>
			<bottomPen lineWidth="0.5" lineColor="#CCCCCC"/>
		</box>
		<conditionalStyle>
			<conditionExpression><![CDATA[new Boolean($V{REPORT_COUNT}.intValue()%2==0)]]></conditionExpression>
			<style backcolor="#F3F7F8"/>
		</conditionalStyle>
	</style>
	<subDataset name="query_contacts">
		<parameter name="p_host_object_id" class="java.lang.Integer"/>
		<parameter name="p_object_name" class="java.lang.String"/>
		<queryString>
			<![CDATA[select a.name1,
  b.alias,
  b.email_address
from icinga_objects a,
  icinga_contacts b,
  icinga_host_contacts c
where a.object_id = b.contact_object_id
and b.contact_id  = c.contact_object_id
and c.host_id     = $P{p_host_object_id}]]>
		</queryString>
		<field name="name1" class="java.lang.String"/>
		<field name="alias" class="java.lang.String"/>
		<field name="email_address" class="java.lang.String"/>
	</subDataset>
	<parameter name="p_host_object_id" class="java.lang.Integer"/>
	<parameter name="p_object_name" class="java.lang.String"/>
	<queryString>
		<![CDATA[select conninfo_id from icinga_conninfo limit 1]]>
	</queryString>
	<field name="conninfo_id" class="java.lang.Long"/>
	<background>
		<band splitType="Stretch"/>
	</background>
	<title>
		<band splitType="Stretch"/>
	</title>
	<pageHeader>
		<band height="40" splitType="Stretch">
			<rectangle radius="2">
				<reportElement x="0" y="5" width="300" height="20" backcolor="#F1F1F1"/>
				<graphicElement>
					<pen lineStyle="Solid" lineColor="#CCCCCC"/>
				</graphicElement>
			</rectangle>
			<staticText>
				<reportElement x="5" y="5" width="120" height="20"/>
				<textElement verticalAlignment="Middle"/>
				<text><![CDATA[Contacts for host:]]></text>
			</staticText>
			<textField>
				<reportElement x="125" y="5" width="100" height="20"/>
				<textElement verticalAlignment="Middle">
					<font isBold="true"/>
				</textElement>
				<textFieldExpression><![CDATA[$P{p_object_name}]]></textFieldExpression>
			</textField>
		</band>
	</pageHeader>
	<detail>
		<band height="20">
			<componentElement>
				<reportElement key="table" style="table" x="0" y="0" width="545" height="20"/>
				<jr:table xmlns:jr="http://jasperreports.sourceforge.net/jasperreports/components" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports/components http://jasperreports.sourceforge.net/xsd/components.xsd">
					<datasetRun subDataset="query_contacts">
						<datasetParameter name="p_host_object_id">
							<datasetParameterExpression><![CDATA[$P{p_host_object_id}]]></datasetParameterExpression>
						</datasetParameter>
						<datasetParameter name="p_object_name">
							<datasetParameterExpression><![CDATA[$P{p_object_name}]]></datasetParameterExpression>
						</datasetParameter>
						<connectionExpression><![CDATA[$P{REPORT_CONNECTION}]]></connectionExpression>
					</datasetRun>
					<jr:column width="110">
						<jr:columnHeader style="table_CH" height="30" rowSpan="1">
							<staticText>
								<reportElement x="5" y="0" width="105" height="30"/>
								<textElement verticalAlignment="Middle"/>
								<text><![CDATA[Contactname]]></text>
							</staticText>
						</jr:columnHeader>
						<jr:detailCell style="table_TD" height="20" rowSpan="1">
							<textField>
								<reportElement x="5" y="0" width="105" height="20"/>
								<textElement verticalAlignment="Middle">
									<font size="8"/>
								</textElement>
								<textFieldExpression><![CDATA[$F{name1}]]></textFieldExpression>
							</textField>
						</jr:detailCell>
					</jr:column>
					<jr:column width="110">
						<jr:columnHeader style="table_CH" height="30" rowSpan="1">
							<staticText>
								<reportElement x="0" y="0" width="110" height="30"/>
								<textElement verticalAlignment="Middle"/>
								<text><![CDATA[Contactalias]]></text>
							</staticText>
						</jr:columnHeader>
						<jr:detailCell style="table_TD" height="20" rowSpan="1">
							<textField>
								<reportElement x="0" y="0" width="110" height="20"/>

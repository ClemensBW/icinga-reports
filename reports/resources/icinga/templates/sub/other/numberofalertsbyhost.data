	<parameter name="p_date_start" class="java.sql.Timestamp"/>
	<parameter name="p_date_end" class="java.sql.Timestamp"/>
	<queryString>
		<![CDATA[select icinga_objects.name1, sum(alerts) as alertsum from (select object_id, count(state_time) as alerts from icinga_statehistory where ((last_hard_state = 1 and state = 1) or (last_hard_state = 2 and state = 2))  and state_type = 1 and state_time BETWEEN $P{p_date_start} AND $P{p_date_end} group by object_id) as states join icinga_objects on states.object_id = icinga_objects.object_id where icinga_objects.is_active=1 and icinga_objects.objecttype_id<=2 group by icinga_objects.name1  order by alertsum desc limit 10;]]>
	</queryString>

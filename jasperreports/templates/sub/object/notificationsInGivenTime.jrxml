		<queryString>
			<![CDATA[select name1,
  start_time,
  state,
  output,
  escalated,
  contacts_notified
from icinga_objects,
  icinga_notifications
where icinga_objects.object_id = icinga_notifications.object_id
and icinga_objects.object_id   = $P{p_object_id}
and start_time between $P{p_date_start} and $P{p_date_end}
order by start_time]]>
		</queryString>

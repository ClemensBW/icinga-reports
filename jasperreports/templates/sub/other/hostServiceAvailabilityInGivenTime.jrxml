<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="Host-Service availability sub report in given time" language="groovy" pageWidth="555" pageHeight="842" columnWidth="555" leftMargin="0" rightMargin="0" topMargin="20" bottomMargin="0">
	<property name="ireport.zoom" value="1.5"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="SUBREPORT_DIR" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA[""]]></defaultValueExpression>
	</parameter>
	<parameter name="p_date_start" class="java.sql.Timestamp">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="p_date_end" class="java.sql.Timestamp">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="p_host_object_id" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
  object_id,
  name1,
  name2,
  objecttype_id,
  ifnull(icinga_availability(
    objects.object_id,
    $P{p_date_start},
    $P{p_date_end}
  ), 0) as sla
FROM
icinga_objects objects
WHERE objects.object_id = $P{p_host_object_id}
union all
SELECT
  object_id,
  name1,
  name2,
  objecttype_id,
  ifnull(icinga_availability(
    objects.object_id,
    $P{p_date_start},
    $P{p_date_end}
  ), 0) as sla
FROM
icinga_objects objects,
icinga_services services
WHERE objects.object_id = services.service_object_id
and services.host_object_id = $P{p_host_object_id}]]>
	</queryString>
	<field name="object_id" class="java.math.BigInteger"/>
	<field name="name1" class="java.lang.String"/>
	<field name="name2" class="java.lang.String"/>
	<field name="objecttype_id" class="java.math.BigInteger"/>
	<field name="sla" class="java.math.BigDecimal"/>
	<background>
		<band splitType="Stretch"/>
	</background>
	<title>
		<band/>
	</title>
	<pageHeader>
		<band height="40" splitType="Stretch">
			<rectangle radius="0">
				<reportElement x="0" y="5" width="300" height="20" backcolor="#F4F4F4"/>
				<graphicElement>
					<pen lineStyle="Solid" lineColor="#CCCCCC"/>
				</graphicElement>
			</rectangle>
			<staticText>
				<reportElement x="5" y="5" width="160" height="20"/>
				<textElement verticalAlignment="Middle"/>
				<text><![CDATA[Host Service Availability Report:]]></text>
			</staticText>
			<textField>
				<reportElement x="165" y="5" width="100" height="20"/>
				<textElement verticalAlignment="Middle">
					<font isBold="true"/>
				</textElement>
				<textFieldExpression><![CDATA[$F{name1}]]></textFieldExpression>
			</textField>
		</band>
	</pageHeader>
	<columnHeader>
		<band height="22" splitType="Stretch">
			<rectangle radius="0">
				<reportElement x="0" y="0" width="545" height="20" backcolor="#3F99AD">
					<printWhenExpression><![CDATA[new Boolean( $V{PAGE_COUNT}.intValue() % 2 ==0 )]]></printWhenExpression>
				</reportElement>
				<graphicElement>
					<pen lineStyle="Solid" lineColor="#CCCCCC"/>
				</graphicElement>
			</rectangle>
			<staticText>
				<reportElement x="5" y="0" width="150" height="20" forecolor="#FFFFFF"/>
				<textElement verticalAlignment="Middle"/>
				<text><![CDATA[Hostname]]></text>
			</staticText>
			<staticText>
				<reportElement x="155" y="0" width="175" height="20" forecolor="#FFFFFF"/>
				<textElement verticalAlignment="Middle"/>
				<text><![CDATA[Servicename]]></text>
			</staticText>
			<staticText>
				<reportElement x="330" y="0" width="100" height="20" forecolor="#FFFFFF"/>
				<textElement verticalAlignment="Middle"/>
				<text><![CDATA[Availability]]></text>
			</staticText>
			<staticText>
				<reportElement x="430" y="0" width="115" height="20" forecolor="#FFFFFF"/>
				<textElement verticalAlignment="Middle"/>
				<text><![CDATA[Availability %]]></text>
			</staticText>
		</band>
	</columnHeader>
	<detail>
		<band height="30" splitType="Stretch">
			<rectangle radius="0">
				<reportElement x="0" y="0" width="545" height="30" backcolor="#FAFAFA">
					<printWhenExpression><![CDATA[new Boolean( $V{PAGE_COUNT}.intValue() % 2 ==0 )]]></printWhenExpression>
				</reportElement>
				<graphicElement>
					<pen lineWidth="0.0" lineStyle="Solid" lineColor="#CCCCCC"/>
				</graphicElement>
			</rectangle>
			<rectangle radius="0">
				<reportElement x="0" y="0" width="545" height="30" backcolor="#E1E1E1">
					<printWhenExpression><![CDATA[new Boolean( $V{PAGE_COUNT}.intValue() % 2 ==1 )]]></printWhenExpression>
				</reportElement>
				<graphicElement>
					<pen lineWidth="0.0" lineStyle="Solid" lineColor="#CCCCCC"/>
				</graphicElement>
			</rectangle>
			<textField>
				<reportElement x="5" y="0" width="150" height="30">
					<printWhenExpression><![CDATA[$F{objecttype_id} == 1]]></printWhenExpression>
				</reportElement>
				<textElement verticalAlignment="Middle">
					<font isBold="true"/>
				</textElement>
				<textFieldExpression><![CDATA[$F{name1}]]></textFieldExpression>
			</textField>
			<textField>
				<reportElement x="155" y="0" width="175" height="30">
					<printWhenExpression><![CDATA[$F{objecttype_id} == 2]]></printWhenExpression>
				</reportElement>
				<textElement verticalAlignment="Middle">
					<font isBold="true"/>
				</textElement>
				<textFieldExpression><![CDATA[$F{name2}]]></textFieldExpression>
			</textField>
			<textField>
				<reportElement x="430" y="0" width="115" height="30"/>
				<textElement verticalAlignment="Middle"/>
				<textFieldExpression><![CDATA[$F{sla} + " %"]]></textFieldExpression>
			</textField>
			<rectangle radius="10">
				<reportElement x="330" y="5" width="50" height="20" backcolor="#00CC00">
					<printWhenExpression><![CDATA[$F{sla} >= 100]]></printWhenExpression>
				</reportElement>
				<graphicElement>
					<pen lineWidth="0.0"/>
				</graphicElement>
			</rectangle>
			<rectangle radius="10">
				<reportElement x="330" y="5" width="50" height="20" backcolor="#CC8000">
					<printWhenExpression><![CDATA[$F{sla} > 99 && $F{sla} < 100]]></printWhenExpression>
				</reportElement>
				<graphicElement>
					<pen lineWidth="0.0"/>
				</graphicElement>
			</rectangle>
			<rectangle radius="10">
				<reportElement x="330" y="5" width="50" height="20" backcolor="#CC0000">
					<printWhenExpression><![CDATA[$F{sla} < 99]]></printWhenExpression>
				</reportElement>
				<graphicElement>
					<pen lineWidth="0.0"/>
				</graphicElement>
			</rectangle>
		</band>
	</detail>
	<columnFooter>
		<band splitType="Stretch"/>
	</columnFooter>
	<pageFooter>
		<band splitType="Stretch"/>
	</pageFooter>
	<summary>
		<band splitType="Stretch"/>
	</summary>
</jasperReport>
